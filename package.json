{
  "name": "barefoot-nomad",
  "version": "1.0.0",
  "description": "An app that enables booking international travel and accommodation easy and convenient for company nomads",
  "main": "index.js",
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test": "NODE_ENV=test yarn undo-migration && yarn migrate && yarn seed && nyc mocha --recursive --exit --require @babel/register --require @babel/polyfill 'src/**/**.test.js' --no-timeout",
    "dev-start": "NODE_ENV=development && nodemon --exec babel-node src/index.js",
    "build": "babel src --out-dir build",
    "start": "node build/index.js",
    "undo-seeding": "npx sequelize-cli db:seed:undo:all",
    "seed": "npx sequelize-cli db:seed:all",
    "undo-migration": "npx sequelize-cli db:migrate:undo:all",
    "migrate": "npx sequelize-cli db:migrate"
  },
  "author": "Andela Simulations Programme",
  "license": "MIT",
  "dependencies": {
    "@sendgrid/mail": "^6.4.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "cross-env": "^5.2.0",
    "dotenv": "^6.0.0",
    "ejs": "^2.6.1",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "express-session": "^1.15.6",
    "jsonwebtoken": "^8.3.0",
    "method-override": "^2.3.10",
    "methods": "^1.1.2",
    "mongoose": "^5.2.2",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "request": "^2.87.0",
    "sequelize": "^5.14.0",
    "sequelize-cli": "^5.5.0",
    "sinon": "^7.4.1",
    "swagger-ui-express": "^4.0.7",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.6",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1"
  }
}
